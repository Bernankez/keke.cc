{
  "name": "keke.cc",
  "type": "module",
  "version": "1.5.7",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@backmoji/vue": "^0.1.1",
    "@bernankez/utils": "^0.6.4",
    "@floating-ui/vue": "^1.1.9",
    "@unocss/reset": "^66.5.12",
    "@vueuse/core": "^14.1.0",
    "@vueuse/math": "^14.1.0",
    "blobity": "^0.2.3",
    "cva": "npm:class-variance-authority@^0.7.1",
    "dayjs": "^1.11.19",
    "nanoid": "^5.1.6",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^4.0.1",
    "@bernankez/theme": "0.2.0",
    "@iconify/json": "^2.2.427",
    "@types/node": "^24.10.7",
    "@unocss/eslint-plugin": "^66.5.12",
    "@vitejs/plugin-basic-ssl": "^2.1.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "eslint-formatter-mo": "^2.4.1",
    "eslint-plugin-format": "^1.2.0",
    "happy-dom": "^20.1.0",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "unocss": "^66.5.12",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bernankez/theme",
      "esbuild",
      "simple-git-hooks",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
