{
  "name": "homepage",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.29.0",
    "lint-staged": "^13.1.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
