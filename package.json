{
  "name": "keke.cc",
  "type": "module",
  "version": "1.1.5",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --open",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint . && pnpm run stylelint",
    "fix": "eslint . --fix && pnpm stylelint:fix",
    "stylelint": "stylelint **/*.{css,vue}",
    "stylelint:fix": "stylelint **/*.{css,vue} --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@unocss/reset": "^0.56.5",
    "@vueuse/core": "^10.5.0",
    "blobity": "^0.2.3",
    "dayjs": "^1.11.10",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.1",
    "@bernankez/eslint-config-unocss": "^0.7.1",
    "@bernankez/stylelint-config": "^0.4.4",
    "@bernankez/theme": "^0.2.0",
    "@iconify/json": "^2.2.131",
    "@types/node": "^20.8.7",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "happy-dom": "^12.9.1",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-ssg": "^0.23.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
