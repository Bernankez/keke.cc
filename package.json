{
  "name": "keke.cc",
  "type": "module",
  "version": "1.5.10",
  "private": true,
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint . -f mo",
    "fix": "eslint . -f mo --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@backmoji/vue": "^0.1.1",
    "@bernankez/utils": "^0.6.4",
    "@floating-ui/vue": "^1.1.10",
    "@unocss/reset": "^66.6.2",
    "@vueuse/core": "^14.2.1",
    "@vueuse/math": "^14.2.1",
    "blobity": "^0.2.3",
    "cva": "npm:class-variance-authority@^0.7.1",
    "dayjs": "^1.11.19",
    "nanoid": "^5.1.6",
    "tailwind-merge": "^3.5.0",
    "vue": "^3.5.29",
    "vue-router": "^5.0.3"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^5.0.1",
    "@bernankez/theme": "0.2.0",
    "@iconify/json": "^2.2.443",
    "@types/node": "^24.10.15",
    "@unocss/eslint-plugin": "^66.6.2",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.2",
    "eslint-formatter-mo": "^2.4.1",
    "eslint-plugin-format": "^2.0.1",
    "happy-dom": "^20.7.0",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "unocss": "^66.6.2",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bernankez/theme",
      "esbuild",
      "simple-git-hooks",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
