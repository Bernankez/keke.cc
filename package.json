{
  "name": "homepage",
  "type": "module",
  "version": "1.0.6",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "vue": "^3.2.45",
    "vue-router": "4"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.2.1",
    "@bernankez/theme": "^0.2.0",
    "@ctrl/tinycolor": "^3.5.0",
    "@iconify/json": "^2.2.35",
    "@types/node": "^18.11.16",
    "@unocss/preset-rem-to-px": "^0.47.6",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.7.0",
    "@vueuse/head": "^1.0.22",
    "bumpp": "^8.2.1",
    "critters": "^0.0.16",
    "eslint": "^8.29.0",
    "lint-staged": "^13.1.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.3",
    "unocss": "^0.47.6",
    "vite": "^4.0.0",
    "vite-ssg": "^0.22.1",
    "vue-tsc": "^1.0.11"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
