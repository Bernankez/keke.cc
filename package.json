{
  "name": "homepage",
  "type": "module",
  "version": "1.0.8",
  "private": true,
  "packageManager": "pnpm@8.4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.4.3",
    "@bernankez/theme": "^0.2.0",
    "@ctrl/tinycolor": "^3.6.0",
    "@iconify/json": "^2.2.62",
    "@types/node": "^20.1.0",
    "@unocss/preset-rem-to-px": "^0.51.12",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "bumpp": "^9.1.0",
    "critters": "^0.0.16",
    "eslint": "^8.40.0",
    "lint-staged": "^13.2.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "vite": "^4.3.5",
    "vite-ssg": "^0.22.2",
    "vue-tsc": "^1.6.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
