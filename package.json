{
  "name": "keke.cc",
  "type": "module",
  "version": "1.1.11",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --open",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint . && pnpm run stylelint",
    "fix": "eslint . --fix && pnpm stylelint:fix",
    "stylelint": "stylelint **/*.{css,vue}",
    "stylelint:fix": "stylelint **/*.{css,vue} --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.3",
    "@vueuse/core": "^10.7.2",
    "blobity": "^0.2.3",
    "dayjs": "^1.11.10",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.4",
    "@bernankez/eslint-config-unocss": "^0.7.4",
    "@bernankez/stylelint-config": "^0.4.4",
    "@bernankez/theme": "^0.2.0",
    "@iconify/json": "^2.2.170",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "happy-dom": "^13.2.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-ssg": "^0.23.6",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
