{
  "name": "keke.cc",
  "type": "module",
  "version": "1.1.2",
  "private": true,
  "packageManager": "pnpm@8.6.6",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "vite --open",
    "build": "vue-tsc && vite-ssg build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . && pnpm run stylelint",
    "fix": "eslint . --fix && pnpm stylelint:fix",
    "stylelint": "stylelint **/*.{css,vue}",
    "stylelint:fix": "stylelint **/*.{css,vue} --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@unocss/reset": "^0.53.4",
    "@vueuse/core": "^10.2.1",
    "blobity": "^0.2.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.0",
    "@bernankez/eslint-config-unocss": "^0.7.0",
    "@bernankez/stylelint-config": "^0.4.3",
    "@bernankez/theme": "^0.2.0",
    "@iconify/json": "^2.2.86",
    "@types/node": "^20.3.3",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "happy-dom": "^9.20.3",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.9.0",
    "typescript": "^5.1.6",
    "unocss": "^0.53.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-vue-devtools": "^0.4.12",
    "vite-ssg": "^0.23.0",
    "vitest": "^0.32.4",
    "vue-tsc": "^1.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
